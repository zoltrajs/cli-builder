{
  "name": "@zoltra/cli-builder",
  "version": "0.1.2",
  "description": "A fast and lightweight CLI builder for TypeScript applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsup",
    "start": "node dist/cli.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "publish": "npm publish --provenance=false --access public"
  },
  "keywords": [
    "cli",
    "typescript",
    "builder",
    "command-line"
  ],
  "author": "Kingrashy12",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.11",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "prompts": "^2.4.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zoltrajs/cli-builder.git"
  },
  "bug": {
    "url": "https://github.com/zoltrajs/cli-builder/issues"
  }
}
